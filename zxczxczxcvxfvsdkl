Tib:AddButton({
    Name = "фарм для свастика",
    Callback = function(value)
args = {
            [1] = "ClearAllTools"
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1Clea1rTool1s"):FireServer(unpack(args))
        
        wait()
        tool = "Balloon"
        repeatCount = 35
      
        player = game.Players.LocalPlayer
        character = player.Character or player.CharacterAdded:Wait()
        humanoidRootPart = character:WaitForChild("HumanoidRootPart")
        humanoid = character:WaitForChild("Humanoid")
      
      humanoid.PlatformStand = true
      
        bodyVelocity = Instance.new("BodyVelocity")
      bodyVelocity.Parent = humanoidRootPart
      bodyVelocity.P = 1250
      bodyVelocity.MaxForce = Vector3.new(4000,4000,4000)
      bodyVelocity.Velocity = Vector3.new(0,2,0)
      humanoidRootPart.CFrame = CFrame.new(8.99999949e+09, -998, 8.99999949e+09, 1, 0, 0, 0, 1, 0, 0, 0, 1)
      humanoidRootPart.Position = Vector3.new(8.99999949e+09, -998, 8.99999949e+09)
      humanoidRootPart.Anchored = true
      humanoidRootPart.Orientation = Vector3.new(0, 0, 0)
      humanoidRootPart.Rotation = Vector3.new(0, 0, 0)
      
        function spawnTool()
            args = {
            [1] = "PickingTools",
            [2] = tool
        }
        game:GetService("ReplicatedStorage").RE:FindFirstChild("1Too1l"):InvokeServer(unpack(args))
      end
      
        function waitForToolToDisappear()
        while game.Players.LocalPlayer.Backpack:FindFirstChild(tool) or character:FindFirstChild(tool) or humanoid:FindFirstChild(tool) do
            wait()
        end
      end
      
        function dupe(veces)
        game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, false)
            startTime = tick()
        for i = 1, veces do
            print(i)
      
                toolInstance = game.Players.LocalPlayer.Backpack:FindFirstChild(tool) 
                or character:FindFirstChild(tool) 
                or humanoid:FindFirstChild(tool)
      
            if not toolInstance then
                spawnTool()
      
                toolInstance = game.Players.LocalPlayer.Backpack:FindFirstChild(tool)
                while not toolInstance do
                    wait()
                    toolInstance = game.Players.LocalPlayer.Backpack:FindFirstChild(tool)
                end
            end
      
      
            toolInstance.Parent = game.Players.LocalPlayer.Backpack
            toolInstance.Parent = humanoid
            toolInstance.Parent = character
      
            waitForToolToDisappear()
      
            if i == veces then
                humanoid.PlatformStand = false
                humanoidRootPart.Anchored = false
                game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, true)
                
            end
        end
        humanoid.Health = 0
            endTime = tick() -- Guardar el tiempo al terminar el bucle
        print("_-----:", endTime - startTime, "sec") -- Calcular y mostrar el tiempo transcurrido
      end
      
      dupe(repeatCount)
	  if i == veces then
wait(10.0)

end
end
})

